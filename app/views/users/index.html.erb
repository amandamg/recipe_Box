
<body>

<h1>List of all the users</h1>
<p><%= link_to 'Create User', new_user_path %></p>
  <% @users.each do |user| %>
    <ul>
      <li><%= user.firstName %> <%= user.lastName %></li>
      <p><%= link_to 'Show', user_path(user) %></p>
    </ul>

  <% end %>

<p><%= link_to 'Back', root_path %></p>

</body>
